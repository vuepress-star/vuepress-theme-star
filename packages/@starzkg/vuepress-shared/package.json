{
  "name": "@starzkg/vuepress-shared",
  "version": "1.0.0-beta.32",
  "description": "Shared files for vuepress-theme-star",
  "keywords": [
    "vuepress-theme-star",
    "vuepress",
    "starzkg"
  ],
  "homepage": "https://vuepress-star.shentuzhigang.cn",
  "bugs": {
    "url": "https://github.com/vuepress-star/vuepress-theme-star/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-star/vuepress-theme-star.git",
    "directory": "packages/@starzkg/vuepress-shared"
  },
  "license": "MIT",
  "author": {
    "name": "Starzkg",
    "email": "shentuzhigang@shentuzhigang.cn",
    "url": "https://shentuzhigang.cn"
  },
  "type": "module",
  "exports": {
    ".": "./lib/node/index.js",
    "./client": "./lib/client/index.js",
    "./styles/": "./lib/client/styles/",
    "./package.json": "./package.json"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "es",
    "lib",
    "src",
    "styles"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "rimraf lib es *.tsbuildinfo",
    "copy": "pnpm copy1 && pnpm copy2",
    "copy1": "cpx \"src/**/*.{css,svg,vue,scss}\" es",
    "copy2": "cpx \"src/**/*.{css,svg,vue,scss}\" lib"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.50",
    "@vuepress/core": "2.0.0-beta.50",
    "@vuepress/plugin-git": "2.0.0-beta.50",
    "@vuepress/plugin-theme-data": "2.0.0-beta.50",
    "@vuepress/shared": "2.0.0-beta.50",
    "@vuepress/utils": "2.0.0-beta.50",
    "chalk": "^5.0.0",
    "copy-to-clipboard": "^3.3.2",
    "dayjs": "^1.11.5",
    "execa": "^6.0.0",
    "ora": "^6.0.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.4"
  },
  "devDependencies": {
    "@types/connect": "3.4.35",
    "@vuepress/bundler-vite": "2.0.0-beta.50",
    "@vuepress/bundler-webpack": "2.0.0-beta.50",
    "@vuepress/core": "2.0.0-beta.50",
    "rimraf": "3.0.2",
    "vite": "3.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
